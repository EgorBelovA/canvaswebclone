#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/annabelova/Desktop/canvas/canvas_app/node_modules/rimraf/node_modules:/Users/annabelova/Desktop/canvas/canvas_app/node_modules:/Users/annabelova/Desktop/canvas/node_modules:/Users/annabelova/Desktop/node_modules:/Users/annabelova/node_modules:/Users/node_modules:/node_modules:/Users/annabelova/Desktop/canvas/canvas_app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/annabelova/Desktop/canvas/canvas_app/node_modules/rimraf/node_modules:/Users/annabelova/Desktop/canvas/canvas_app/node_modules:/Users/annabelova/Desktop/canvas/node_modules:/Users/annabelova/Desktop/node_modules:/Users/annabelova/node_modules:/Users/node_modules:/node_modules:/Users/annabelova/Desktop/canvas/canvas_app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
